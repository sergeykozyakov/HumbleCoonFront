{
  "name": "humble-coon",
  "version": "1.0.0",
  "description": "Humble Coon client side",
  "scripts": {
    "start": "webpack-dev-server --config webpack/config.dev.js",
    "build": "webpack --config webpack/config.prod.js",
    "server": "node server/server.js dev",
    "lint": "eslint src/**/*.[jt]s* tests/**/*.[jt]s* --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergeykozyakov/HumbleCoonFront.git"
  },
  "keywords": [
    "humble",
    "coon"
  ],
  "author": "Sergey Kozyakov",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/sergeykozyakov/HumbleCoonFront/issues"
  },
  "homepage": "https://github.com/sergeykozyakov/HumbleCoonFront#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "npm run lint",
      "git add"
    ]
  },
  "browserslist": [
    "> .2%",
    "IE 10"
  ],
  "dependencies": {
    "@hot-loader/react-dom": "16.14.0",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "fontsource-roboto": "4.0.0",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.13.0",
    "@types/jest": "26.0.22",
    "@types/react": "16.14.5",
    "@types/react-dom": "16.9.12",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.10.0",
    "css-loader": "5.2.0",
    "eslint": "7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.0",
    "html-webpack-plugin": "4.5.2",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-cli": "26.1.0",
    "lint-staged": "10.2.11",
    "react-hot-loader": "4.13.0",
    "style-loader": "2.0.0",
