{
  "compilerOptions": {
    "module": "umd",
    "lib": [
      "ESNext",
      "DOM.Iterable",
      "DOM"
    ],
    "jsx": "react",
    "noEmit": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
